- var sectionTitle = data["open-source"].sectionTitle
include ../section-title/section-title.pug

.column(span="row" height="10")
.column(span="row")
   h3.open-source__moreTitle= data["open-source"].stat.titluMult
.column(span="row" height="10")
.column(span="row")
  ul.stats__box
    each item in data["open-source"].stat.itemList
       li(class=`stats_list`)
         span(class=`icon__${item.icon}`)(class=`iconite`)
         span 
            each label in item.label
              span(class='casute')=label
.column(span="row" height="10")
.column(span="row")
  h3.open-source__secondTitle= data["open-source"].stat.secondTitle
.column(span="row")
  ul.listaGit 
    each a in data['open-source'].stat.itemGit
      li.eleGit
        span.cacaunu=a.labelTitle
        span.cacadoi=a.labelText 
        span.cacatrei=a.progressBar 
        .git__projects=data['open-source'].projects.project.projectName
          each g, key in data['open-source'].projects.project
            input(
              type="checkbox"
              name= "git__pink-icons"
              id=(`project__checkbox--${key}`)).project__checkbox
            label(for=`project__chevron=${key}`).project__chevron
              .icon__info
.column(span="row" height="10")
.column(span="row")
  button.button__disc--green
    i.icon__arrow-down
.column(span="row" height="10")
